{
  "mcpServers": {
    "brave-search": {
      "command": "fish",
      "args": [
        "-c",
        "npx -y @modelcontextprotocol/server-brave-search"
      ],
      "env": {
        "_BRAVE_API_KEY": "API_KEY"
      }
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/vlad"
      ]
    },
    "memory": {
      "command": "fish",
      "args": [
        "-c",
        "npx -y @modelcontextprotocol/server-memory"
      ],
      "env": {
        "_MEMORY_FILE_PATH": "path"
      }
    },
    "youtube": {
      "command": "fish",
      "args": ["-c", "npx -y @modelcontextprotocol/server-youtube"],
      "env": {
        "_YOUTUBE_API_KEY": "<YOUR_TOKEN>"
      }
    },
    "github": {
      "command": "fish",
      "args": [
        "-—Å",
        "npx -y @modelcontextprotocol/server-github"
      ],
      "env": {
        "_GITHUB_PERSONAL_ACCESS_TOKEN": "<YOUR_TOKEN>"
      }
    },
    "puppeteer": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-puppeteer"]
    }
  }
}